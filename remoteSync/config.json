[
   {
      "appid": "leads",
      "fallback": "core",
      "version": "1.0",
      "appName": "leads application",
      "syncType": "merge",
      "src": "https://github.com/ghadad/pele4u/blob/v117.9_remote_code/remoteSync/apps/lead.zip",
      "states": [
         {
            "state": "app.lead",
            "url": "/lead",
            "views": {
               "menuContent": {
                  "templateUrl": function(){ return 'app/apps/lead/lead.html'; },
                  "controller": "leadCtrl"
               }
            },
            "src": [
               "app/apps/lead/leadCtrl.js"
            ]
         },
         {
            "state": "app.lead.report",
            "url": "/report",
            "views": {
               "menuContent@app": {
                  "templateUrl": function(){ return 'app/apps/lead/report.html'; },
                  "controller": "reportCtrl"
               }
            },
            "src": [
               "app/apps/lead/reportCtrl.js"
            ]
         }
      ]
   }
]
