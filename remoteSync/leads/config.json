   {
      "appid": "leads",
      "fallback": "core",
      "version": "1.0",
      "appName": "leads application",
      "syncType": "merge",
      "copyRootApp":true,
      "src": "https://github.com/ghadad/pele4u/blob/v117.9_remote_code/remoteSync/leads/www.zip",
      "manifest":"manifest.json",
      "states": [
         {
            "state": "app.leads",
            "url": "/lead",
            "views": {
               "menuContent": {
                  "templateUrl": "app/apps/leads/lead.html",
                  "controller": "leadCtrl"
               }
            },
            "src": [
               "app/apps/leads/leadCtrl.js"
            ]
         },
         {
            "state": "app.leads.report",
            "url": "/report",
            "views": {
               "menuContent@app": {
                  "templateUrl": "app/apps/leads/report.html",
                  "controller": "reportCtrl"
               }
            },
            "src": [
               "app/apps/leads/reportCtrl.js"
            ]
         }
      ]
   }
